<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <link rel="stylesheet" href="/assets/css/Navigation-with-Search.css">
  <link rel="stylesheet" href="/assets/css/Navigation-Clean.css">
  <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="/assets/fonts/ionicons.css">
  <link rel="stylesheet" href="/assets/ionicons.min.css">
  <link rel="stylesheet" href="/assets/css/member-area.css">
  <link rel="stylesheet" href="/assets/css/Header-Blue.css">
  <link rel="stylesheet" href="/assets/css/company-info.css">
  <link rel="stylesheet" href="/assets/css/index.css">


</head>

<body>


  <section style="height: auto;margin-bottom: 20px;">
    <div class="row d-flex justify-content-center" style="margin-top: 80px;">
      <div class="col-lg-8 offset-lg-0">
        <div class="card" style='height: 650px; max-height:750px;'>
          <div class="card-body">
            <div class="d-flex flex-row align-items-center" style="height: 50px; justify-content: flex-start">
              <div>
                <a id='ticker-text' class="navbar-brand" href="#"
                  style='color: blue; font-weight: bold; font-size:1.8rem'>{{price.symbol}}</a>

              </div>
              <div>
                {{#if posChange}}
                <span
                  style='margin-left: 5px;color: rgb(22, 158, 22); font-weight: bold; font-size:1.6rem'>(+{{posChange}}%)</span>
                {{/if}}
                {{#if negChange}}
                <span
                  style='margin-left: 5px;color: rgb(238, 27, 27); font-weight: bold; font-size:1.6rem'>({{negChange}}%)</span>
                {{/if}}
              </div>
              <div style='margin-left: 5px;'>
                <a class="btn action-button" style='background-color: #56c6c6' role="button" href="#"><svg width='15px'
                    height='15px' xmlns="http://www.w3.org/2000/svg" class="ionicon" viewBox="0 0 512 512">
                    <title>Bookmark</title>
                    <path d="M352 48H160a48 48 0 00-48 48v368l144-128 144 128V96a48 48 0 00-48-48z" fill="none"
                      stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" />
                  </svg></a>
              </div>

            </div>
            {{#if price.shortName}}
            <h5 class="text-muted card-subtitle mb-2">{{price.shortName}}</h5>
            {{else}}
            <h5 class="text-muted card-subtitle mb-2">{{price.longName}}</h5>
            {{/if}}
            <div class="d-flex flex-row justify-content-center"
              style="height: 45px;margin-bottom: 30px; margin-top:30px; width:100%">
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                {{#if foundedDate}}
                <h6 class="text-center text-dark mb-2" style="color: black; font-weight:600">Fundada</h6>
                <h6 class="text-center text-muted mb-2">{{foundedDate}}</h6>
                {{else}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Pais</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.country}}</h6>
                {{/if}}
              </div>
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                {{#if sumProf.website}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Website</h6>
                <h6 class="text-center text-muted mb-2"><a href='{{sumProf.website}}'
                    target='_blank'>{{sumProf.website}}</a></h6>
                {{else}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Cidade</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.city}}</h6>
                {{/if}}
              </div>
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                {{#if sumProf.fullTimeEmployees}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Nº de empregados</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.fullTimeEmployees}}</h6>
                {{else}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Estado</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.state}}</h6>
                {{/if}}
              </div>
            </div>

            <div class="d-flex flex-row justify-content-center" style="height: 45px;margin-top: 30px; width:100%">
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Sede</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.city}}</h6>
              </div>
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                {{#if exchange}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Listada em</h6>
                <h6 class="text-center text-muted mb-2">{{exchange}}</h6>
                {{else}}

                {{/if}}
              </div>
              <div class="d-flex flex-column" style="height: 45px;width: 30%;">
                {{#if sumProf.address2}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Endereco</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.address2}}</h6>
                {{else}}
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Endereco</h6>
                <h6 class="text-center text-muted mb-2">{{sumProf.address1}}</h6>
                {{/if}}
              </div>
            </div>
            <div class="d-flex flex-row justify-content-between" style="height: 45px;width: 100%;margin-top: 30px;">
              {{#if sumProf.industry}}
              <div class="d-flex flex-column" style="height: 45px;width: 50%;">
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Industria</h6>
                <h6 class="text-nowrap text-center text-muted mb-2">{{sumProf.industry}}</h6>
              </div>
              {{else}}
              <div class="d-flex flex-column" style="height: 45px;width: 50%;">
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Industria</h6>
                <h6 class="text-nowrap text-center text-muted mb-2">Outros</h6>
              </div>
              {{/if}}
              {{#if sumProf.sector}}
              <div class="d-flex flex-column" style="height: 45px;width: 50%;">
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Setor</h6>
                <h6 class="text-nowrap text-center text-muted mb-2">{{sumProf.sector}}</h6>
              </div>
              {{else}}
              <div class="d-flex flex-column" style="height: 45px;width: 50%;">
                <h6 class="text-center text-dark mb-2" style="color: black;font-weight:600">Setor</h6>
                <h6 class="text-nowrap text-left text-muted mb-2">Outros</h6>
              </div>
              {{/if}}
            </div>
            <div class="d-flex flex-column just" style="height: 45px;width: 90%;margin-top: 40px;margin-bottom: 20px;">
              <h6 style='font-weight:bold'>Descricao</h6>
              <p class='text-start text-muted mb-2'>{{sumProf.longBusinessSummary}}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div style="height: 60px;margin-bottom: 30px;"></div>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="d-flex flex-column justify-content-center align-items-center" style="height: auto;">
          <div class="d-flex flex-row justify-content-center align-items-center align-content-center"
            style="height: 50px;width: 225px;background: transparent;">
            <button id='btnEstatisticas' style='padding: 6px 12px' class="btn btn-primary border rounded-pill"
              type="button">Estatisticas</button>
          </div>
          <div class="table-responsive border-light shadow" style="width: 70%;margin-top: 15px;">
            <table class="table tabela-infos">
              <thead class='tabela-infos-head'>
                <tr>
                  <th class='primeira-coluna'></th>
                  <th>Shares Outs</th>
                  <th>Mkt Cap</th>
                  <th>Day Range</th>
                  <th>Volume</th>
                  <th>Volume Med</th>
                  <th>200d Med</th>
                  <th>52w Range</th>
                  <th>Beta</th>
                  <th>EPS</th>
                </tr>
              </thead>
              <tbody class='tabela-infos-body'>
                <tr>
                  <td class='primeira-coluna'>{{price.symbol}}</td>
                  <td class='segunda-coluna'>{{defKey.sharesOutstanding}}M</td>
                  {{#if price.marketCap}}
                  <td class='segunda-coluna'>{{price.marketCap}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>{{price.regularMarketPreviousClose}}M</td>
                  {{/if}}

                  {{#if sumDet.regularMarketDayHigh}}
                  <td class='segunda-coluna'>{{sumDet.regularMarketDayLow}} - {{sumDet.regularMarketDayHigh}}</td>
                  {{else}}
                  <td class='segunda-coluna'>{{price.regularMarketDayLow}} - {{price.regularMarketDayHigh}}</td>
                  {{/if}}

                  {{#if sumDet.volume}}
                  <td class='segunda-coluna'>{{sumDet.volume}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>{{price.regularMarketVolume}}M</td>
                  {{/if}}

                  {{#if sumDet.averageDailyVolume10Day}}
                  <td class='segunda-coluna'>{{sumDet.averageDailyVolume10Day}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>{{price.averageDailyVolume3Month}}M</td>
                  {{/if}}

                  {{#if twoHundredDayAverage}}
                  <td class='segunda-coluna'>{{twoHundredDayAverage}}</td>
                  {{else}}
                  <td class='segunda-coluna'>{{sumDet.fiftyDayAverage}}NaN</td>
                  {{/if}}

                  {{#if fiftyTwoWeekHigh}}
                  <td class='segunda-coluna'>{{fiftyTwoWeekLow}} - {{fiftyTwoWeekHigh}}</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}


                  {{#if beta}}
                  <td class='segunda-coluna'>{{beta}}</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if defKey.trailingEps}}
                  <td class='segunda-coluna'>{{defKey.trailingEps}}</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}
                </tr>
              </tbody>
            </table>
          </div>
          <div class="d-flex flex-row justify-content-center align-items-center align-content-center"
            style="height: 50px;width: 225px;background: transparent; margin-top:40px;">
            <button id='btnIndicadores' style='padding: 6px 12px' class="btn btn-primary border rounded-pill"
              type="button">Indicadores</button>
          </div>
          <div class="table-responsive border-light shadow" style="width: 70%;margin-top: 15px;">
            <table class="table tabela-infos">
              <thead class='tabela-infos-head'>
                <tr>
                  <th class='primeira-coluna'></th>
                  <th class='segunda-coluna-header'>Receita</th>
                  <th class='terceira-coluna-header'>EBITDA</th>
                  <th class='quarta-coluna-header'>Lucro Bruto</th>
                  <th class='quinta-coluna-header'>FC Livre</th>
                  <th class='sexta-coluna-header'>FC operacional</th>
                  <th class='setima-coluna-header'>Dívida Total</th>
                  <th class='oitava-coluna-header'>Dívida/Equity</th>
                  <th class='nona-coluna-header'>ROA</th>
                  <th class='decima-coluna-header'>ROE</th>
                  <th class='decimaprimeira-coluna-header'>M. Operacional</th>
                  <th class='decimasegunda-coluna-header'>M. Lucro</th>
                </tr>
              </thead>
              <tbody class='tabela-infos-body'>
                <tr>
                  <td class='primeira-coluna'>{{price.symbol}}</td>
                  <td class='segunda-coluna'>{{finData.totalRevenue}}M</td>
                  {{#if finData.ebitda}}
                  <td class='segunda-coluna'>{{finData.ebitda}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.grossProfits}}
                  <td class='segunda-coluna'>{{finData.grossProfits}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.freeCashflow}}
                  <td class='segunda-coluna'>{{finData.freeCashflow}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.operatingCashflow}}
                  <td class='segunda-coluna'>{{finData.operatingCashflow}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.totalDebt}}
                  <td class='segunda-coluna'>{{finData.totalDebt}}M</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.debtToEquity}}
                  <td class='segunda-coluna'>{{finData.debtToEquity}}</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}


                  {{#if finData.returnOnAssets}}
                  <td class='segunda-coluna'>{{finData.returnOnAssets}}%</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.returnOnEquity}}
                  <td class='segunda-coluna'>{{finData.returnOnEquity}}%</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.operatingMargins}}
                  <td class='segunda-coluna'>{{finData.operatingMargins}}%</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                  {{#if finData.profitMargins}}
                  <td class='segunda-coluna'>{{finData.profitMargins}}%</td>
                  {{else}}
                  <td class='segunda-coluna'>NaN</td>
                  {{/if}}

                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </section>


  <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script src='/assets/js/autocomplete.js'></script>
  <script src='/assets/js/charts.js'></script>


</body>

</html>